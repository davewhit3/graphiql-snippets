#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/dbialy/projects/js/graphiql-snippets/example/node_modules/.pnpm/graphql-language-service@5.3.1_graphql@16.11.0/node_modules/graphql-language-service/dist/node_modules:/Users/dbialy/projects/js/graphiql-snippets/example/node_modules/.pnpm/graphql-language-service@5.3.1_graphql@16.11.0/node_modules/graphql-language-service/node_modules:/Users/dbialy/projects/js/graphiql-snippets/example/node_modules/.pnpm/graphql-language-service@5.3.1_graphql@16.11.0/node_modules:/Users/dbialy/projects/js/graphiql-snippets/example/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/dbialy/projects/js/graphiql-snippets/example/node_modules/.pnpm/graphql-language-service@5.3.1_graphql@16.11.0/node_modules/graphql-language-service/dist/node_modules:/Users/dbialy/projects/js/graphiql-snippets/example/node_modules/.pnpm/graphql-language-service@5.3.1_graphql@16.11.0/node_modules/graphql-language-service/node_modules:/Users/dbialy/projects/js/graphiql-snippets/example/node_modules/.pnpm/graphql-language-service@5.3.1_graphql@16.11.0/node_modules:/Users/dbialy/projects/js/graphiql-snippets/example/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir//bin/node" ]; then
  exec "$basedir//bin/node"  "$basedir/../graphql-language-service/dist/temp-bin.js" "$@"
else
  exec /bin/node  "$basedir/../graphql-language-service/dist/temp-bin.js" "$@"
fi
